<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Students List</title>
</head>
<body>
    <h1>Students List</h1>
    <table border="1">
        <thead>
            <tr>
                <th>Full Name</th>
                <th>Email</th>
                <th>Roll Number</th>
                <th>Class</th>
                <th>SID</th>
                <th>Barcode</th>
            </tr>
        </thead>
        <tbody>
            <% students.forEach(student => { %>
                <tr>
                    <td><%= student.fullname %></td>
                    <td><%= student.email %></td>
                    <td><%= student.roll %></td>
                    <td><%= student.classs %></td>
                    <td><%= student.sid %></td>
                    <td><img src="<%= student.barcode %>" alt="Barcode" /></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
